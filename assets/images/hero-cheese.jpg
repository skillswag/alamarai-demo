<svg width="1920" height="1080" xmlns="http://www.w3.org/2000/svg">
  <!-- Hero Background: Cheese Products -->
  <defs>
    <linearGradient id="heroGrad3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fafafa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f5f5f5;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1920" height="1080" fill="url(#heroGrad3)"/>
  
  <!-- Decorative elements -->
  <circle cx="350" cy="250" r="160" fill="#006633" opacity="0.08"/>
  <circle cx="1550" cy="750" r="200" fill="#ffeb3b" opacity="0.15"/>
  <circle cx="1050" cy="350" r="110" fill="#006633" opacity="0.08"/>
  
  <!-- Cheese package illustration -->
  <g transform="translate(700, 280)">
    <!-- Package box -->
    <rect x="80" y="80" width="360" height="480" rx="15" fill="#ffffff" stroke="#006633" stroke-width="5"/>
    <!-- Top band -->
    <rect x="80" y="80" width="360" height="120" rx="15" fill="#006633"/>
    <!-- Logo area -->
    <text x="260" y="140" font-family="Arial" font-size="56" font-weight="bold" fill="#ffffff" text-anchor="middle">ALMARAI</text>
    <!-- Product info -->
    <text x="260" y="280" font-family="Arial" font-size="52" font-weight="bold" fill="#006633" text-anchor="middle">Cheddar</text>
    <text x="260" y="340" font-family="Arial" font-size="52" font-weight="bold" fill="#006633" text-anchor="middle">Cheese</text>
    <text x="260" y="400" font-family="Arial" font-size="28" fill="#666666" text-anchor="middle">Sliced</text>
    <!-- Cheese illustration -->
    <ellipse cx="260" cy="480" rx="80" ry="20" fill="#ffeb3b" opacity="0.6"/>
    <path d="M 180 460 Q 260 440 340 460" fill="none" stroke="#ffeb3b" stroke-width="3" opacity="0.6"/>
  </g>
  
  <!-- Text overlay -->
  <text x="960" y="950" font-family="Arial" font-size="36" font-weight="bold" fill="#006633" text-anchor="middle" opacity="0.7">AI-Generated Product Visual</text>
</svg>
