<svg width="1920" height="1080" xmlns="http://www.w3.org/2000/svg">
  <!-- Hero Background: Fresh Juice -->
  <defs>
    <linearGradient id="heroGrad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fff3e0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffe0b2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1920" height="1080" fill="url(#heroGrad2)"/>
  
  <!-- Decorative elements -->
  <circle cx="400" cy="300" r="180" fill="#ff9800" opacity="0.2"/>
  <circle cx="1500" cy="700" r="220" fill="#ff9800" opacity="0.2"/>
  <circle cx="1100" cy="300" r="120" fill="#006633" opacity="0.1"/>
  
  <!-- Juice carton illustration -->
  <g transform="translate(760, 240)">
    <!-- Carton body -->
    <path d="M 80 80 L 320 80 L 320 600 L 80 600 Z" fill="#ffffff" stroke="#006633" stroke-width="4"/>
    <!-- Top flap -->
    <path d="M 80 80 L 200 20 L 320 80" fill="#006633"/>
    <!-- Orange gradient panel -->
    <rect x="100" y="200" width="200" height="300" rx="15" fill="#ff9800" opacity="0.3"/>
    <!-- Label -->
    <text x="200" y="280" font-family="Arial" font-size="48" font-weight="bold" fill="#006633" text-anchor="middle">ALMARAI</text>
    <text x="200" y="340" font-family="Arial" font-size="36" fill="#006633" text-anchor="middle">Orange</text>
    <text x="200" y="380" font-family="Arial" font-size="36" fill="#006633" text-anchor="middle">Juice</text>
    <text x="200" y="440" font-family="Arial" font-size="20" fill="#666666" text-anchor="middle">100% Natural</text>
    <!-- Orange slices decoration -->
    <circle cx="140" cy="480" r="30" fill="#ff9800" opacity="0.7"/>
    <circle cx="200" cy="500" r="35" fill="#ff9800" opacity="0.7"/>
    <circle cx="260" cy="480" r="30" fill="#ff9800" opacity="0.7"/>
  </g>
  
  <!-- Text overlay -->
  <text x="960" y="950" font-family="Arial" font-size="36" font-weight="bold" fill="#006633" text-anchor="middle" opacity="0.7">AI-Generated Product Visual</text>
</svg>
