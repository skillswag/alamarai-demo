<svg width="800" height="800" xmlns="http://www.w3.org/2000/svg">
  <!-- Product: Fresh Full Cream Milk -->
  <rect width="800" height="800" fill="#f8f9fa"/>
  
  <!-- Shadow -->
  <ellipse cx="400" cy="750" rx="150" ry="20" fill="#000000" opacity="0.1"/>
  
  <!-- Milk bottle -->
  <g transform="translate(200, 100)">
    <!-- Bottle body -->
    <rect x="80" y="120" width="240" height="450" rx="20" fill="#ffffff" stroke="#e0e0e0" stroke-width="3"/>
    <!-- Cap -->
    <rect x="120" y="80" width="160" height="50" rx="10" fill="#006633"/>
    <!-- Label -->
    <rect x="100" y="250" width="200" height="200" rx="10" fill="#006633"/>
    <text x="200" y="310" font-family="Arial" font-size="36" font-weight="bold" fill="#ffffff" text-anchor="middle">ALMARAI</text>
    <text x="200" y="355" font-family="Arial" font-size="24" fill="#ffffff" text-anchor="middle">Fresh Milk</text>
    <text x="200" y="390" font-family="Arial" font-size="18" fill="#ffffff" text-anchor="middle">Full Cream</text>
    <text x="200" y="425" font-family="Arial" font-size="16" fill="#ffffff" text-anchor="middle">1 Liter</text>
  </g>
  
  <!-- AI Badge -->
  <rect x="620" y="20" width="160" height="40" rx="20" fill="#006633"/>
  <text x="700" y="45" font-family="Arial" font-size="16" font-weight="bold" fill="#ffffff" text-anchor="middle">AI Generated</text>
</svg>
