<svg width="800" height="800" xmlns="http://www.w3.org/2000/svg">
  <!-- Product: Cheddar Cheese -->
  <rect width="800" height="800" fill="#f8f9fa"/>
  
  <!-- Shadow -->
  <ellipse cx="400" cy="750" rx="160" ry="20" fill="#000000" opacity="0.1"/>
  
  <!-- Cheese package -->
  <g transform="translate(160, 140)">
    <!-- Package box -->
    <rect x="80" y="80" width="320" height="420" rx="12" fill="#ffffff" stroke="#e0e0e0" stroke-width="3"/>
    <!-- Top band -->
    <rect x="80" y="80" width="320" height="100" rx="12" fill="#006633"/>
    <!-- Logo -->
    <text x="240" y="135" font-family="Arial" font-size="42" font-weight="bold" fill="#ffffff" text-anchor="middle">ALMARAI</text>
    <!-- Product info -->
    <text x="240" y="260" font-family="Arial" font-size="44" font-weight="bold" fill="#006633" text-anchor="middle">Cheddar</text>
    <text x="240" y="310" font-family="Arial" font-size="44" font-weight="bold" fill="#006633" text-anchor="middle">Cheese</text>
    <text x="240" y="360" font-family="Arial" font-size="22" fill="#666666" text-anchor="middle">Sliced</text>
    <text x="240" y="400" font-family="Arial" font-size="20" fill="#666666" text-anchor="middle">200g</text>
    <!-- Cheese illustration -->
    <ellipse cx="240" cy="450" rx="60" ry="15" fill="#ffeb3b" opacity="0.5"/>
  </g>
  
  <!-- AI Badge -->
  <rect x="620" y="20" width="160" height="40" rx="20" fill="#006633"/>
  <text x="700" y="45" font-family="Arial" font-size="16" font-weight="bold" fill="#ffffff" text-anchor="middle">AI Generated</text>
</svg>
