<svg width="1000" height="1000" xmlns="http://www.w3.org/2000/svg">
  <!-- Gallery Scene 3: Breakfast Essentials -->
  <defs>
    <linearGradient id="sceneGrad3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fafafa;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f5f5f5;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1000" height="1000" fill="url(#sceneGrad3)"/>
  
  <!-- Table with wood texture suggestion -->
  <rect x="0" y="550" width="1000" height="450" fill="#deb887" opacity="0.3"/>
  <rect x="0" y="600" width="1000" height="400" fill="#f5f5f5" opacity="0.5"/>
  
  <!-- Milk bottle (left) -->
  <g transform="translate(120, 250)">
    <rect x="40" y="100" width="160" height="330" rx="18" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="70" y="70" width="100" height="40" rx="10" fill="#006633"/>
    <rect x="60" y="220" width="140" height="140" rx="10" fill="#006633"/>
    <text x="130" y="280" font-family="Arial" font-size="28" font-weight="bold" fill="#ffffff" text-anchor="middle">MILK</text>
  </g>
  
  <!-- Yogurt cup (center-left) -->
  <g transform="translate(380, 370)">
    <ellipse cx="70" cy="230" rx="80" ry="22" fill="#e0e0e0"/>
    <rect x="-10" y="90" width="160" height="140" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <ellipse cx="70" cy="90" rx="80" ry="22" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="10" y="130" width="120" height="80" rx="8" fill="#e91e63"/>
    <text x="70" y="175" font-family="Arial" font-size="18" font-weight="bold" fill="#ffffff" text-anchor="middle">YOGURT</text>
  </g>
  
  <!-- Cheese package (center-right) -->
  <g transform="translate(580, 340)">
    <rect x="20" y="70" width="200" height="280" rx="12" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="20" y="70" width="200" height="80" rx="12" fill="#006633"/>
    <text x="120" y="115" font-family="Arial" font-size="24" font-weight="bold" fill="#ffffff" text-anchor="middle">CHEESE</text>
  </g>
  
  <!-- Glass of milk (right) -->
  <g transform="translate(820, 420)">
    <!-- Glass -->
    <path d="M 30 100 L 50 300 L 110 300 L 130 100 Z" fill="#ffffff" stroke="#e0e0e0" stroke-width="2" opacity="0.9"/>
    <ellipse cx="80" cy="100" rx="50" ry="15" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
  </g>
  
  <!-- Morning sun decoration -->
  <circle cx="150" cy="150" r="50" fill="#ffc107" opacity="0.3"/>
  <circle cx="150" cy="150" r="70" fill="#ffc107" opacity="0.1"/>
  
  <!-- Text overlay -->
  <text x="500" y="900" font-family="Arial" font-size="32" font-weight="bold" fill="#006633" text-anchor="middle">Breakfast Essentials</text>
  <text x="500" y="950" font-family="Arial" font-size="20" fill="#666666" text-anchor="middle">AI-Generated Morning Scene</text>
</svg>
