<svg width="1000" height="1000" xmlns="http://www.w3.org/2000/svg">
  <!-- Gallery Scene 2: Fresh Juice Range -->
  <defs>
    <linearGradient id="sceneGrad2" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#fff3e0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1000" height="1000" fill="url(#sceneGrad2)"/>
  
  <!-- Table surface -->
  <rect x="0" y="650" width="1000" height="350" fill="#f5f5f5" opacity="0.7"/>
  
  <!-- Juice carton 1 (left) -->
  <g transform="translate(200, 300)">
    <path d="M 40 60 L 180 60 L 180 400 L 40 400 Z" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <path d="M 40 60 L 110 20 L 180 60" fill="#ff9800"/>
    <rect x="60" y="160" width="100" height="180" rx="10" fill="#ff9800" opacity="0.3"/>
    <text x="110" y="240" font-family="Arial" font-size="22" font-weight="bold" fill="#006633" text-anchor="middle">ORANGE</text>
    <circle cx="110" cy="320" r="30" fill="#ff9800" opacity="0.6"/>
  </g>
  
  <!-- Juice carton 2 (center) -->
  <g transform="translate(420, 280)">
    <path d="M 40 60 L 180 60 L 180 420 L 40 420 Z" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <path d="M 40 60 L 110 20 L 180 60" fill="#ffc107"/>
    <rect x="60" y="180" width="100" height="180" rx="10" fill="#ffc107" opacity="0.3"/>
    <text x="110" y="260" font-family="Arial" font-size="22" font-weight="bold" fill="#006633" text-anchor="middle">MANGO</text>
    <ellipse cx="110" cy="340" rx="25" ry="35" fill="#ffc107" opacity="0.6"/>
  </g>
  
  <!-- Juice carton 3 (right) -->
  <g transform="translate(640, 320)">
    <path d="M 40 60 L 180 60 L 180 380 L 40 380 Z" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <path d="M 40 60 L 110 20 L 180 60" fill="#e91e63"/>
    <rect x="60" y="150" width="100" height="180" rx="10" fill="#e91e63" opacity="0.3"/>
    <text x="110" y="230" font-family="Arial" font-size="20" font-weight="bold" fill="#006633" text-anchor="middle">MIXED</text>
    <circle cx="90" cy="300" r="20" fill="#e91e63" opacity="0.6"/>
    <circle cx="130" cy="310" r="15" fill="#ffc107" opacity="0.6"/>
  </g>
  
  <!-- Decorative fruit elements -->
  <circle cx="120" cy="180" r="35" fill="#ff9800" opacity="0.2"/>
  <circle cx="850" cy="220" r="45" fill="#ffc107" opacity="0.2"/>
  
  <!-- Text overlay -->
  <text x="500" y="900" font-family="Arial" font-size="32" font-weight="bold" fill="#006633" text-anchor="middle">Fresh Juice Range</text>
  <text x="500" y="950" font-family="Arial" font-size="20" fill="#666666" text-anchor="middle">AI-Generated Product Display</text>
</svg>
