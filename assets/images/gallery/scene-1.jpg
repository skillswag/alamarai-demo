<svg width="1000" height="1000" xmlns="http://www.w3.org/2000/svg">
  <!-- Gallery Scene 1: Premium Dairy Collection -->
  <defs>
    <linearGradient id="sceneGrad1" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#e8f5e9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1000" height="1000" fill="url(#sceneGrad1)"/>
  
  <!-- Table surface -->
  <rect x="0" y="600" width="1000" height="400" fill="#f5f5f5" opacity="0.7"/>
  
  <!-- Product 1: Milk bottle (left) -->
  <g transform="translate(150, 300)">
    <rect x="30" y="80" width="140" height="280" rx="15" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="60" y="50" width="80" height="35" rx="8" fill="#006633"/>
    <rect x="50" y="160" width="120" height="120" rx="8" fill="#006633"/>
    <text x="110" y="210" font-family="Arial" font-size="24" font-weight="bold" fill="#ffffff" text-anchor="middle">MILK</text>
  </g>
  
  <!-- Product 2: Yogurt cup (center) -->
  <g transform="translate(400, 380)">
    <ellipse cx="80" cy="250" rx="90" ry="25" fill="#e0e0e0"/>
    <rect x="-10" y="100" width="180" height="150" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <ellipse cx="80" cy="100" rx="90" ry="25" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="10" y="150" width="140" height="90" rx="8" fill="#006633"/>
    <text x="80" y="195" font-family="Arial" font-size="20" font-weight="bold" fill="#ffffff" text-anchor="middle">YOGURT</text>
  </g>
  
  <!-- Product 3: Cheese pack (right) -->
  <g transform="translate(650, 340)">
    <rect x="20" y="60" width="180" height="260" rx="10" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="20" y="60" width="180" height="70" rx="10" fill="#006633"/>
    <text x="110" y="100" font-family="Arial" font-size="20" font-weight="bold" fill="#ffffff" text-anchor="middle">CHEESE</text>
  </g>
  
  <!-- Decorative elements -->
  <circle cx="100" cy="150" r="40" fill="#006633" opacity="0.1"/>
  <circle cx="850" cy="200" r="60" fill="#006633" opacity="0.1"/>
  
  <!-- Text overlay -->
  <text x="500" y="900" font-family="Arial" font-size="32" font-weight="bold" fill="#006633" text-anchor="middle">Premium Dairy Collection</text>
  <text x="500" y="950" font-family="Arial" font-size="20" fill="#666666" text-anchor="middle">AI-Generated Scene</text>
</svg>
