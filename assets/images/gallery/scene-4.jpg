<svg width="1000" height="1000" xmlns="http://www.w3.org/2000/svg">
  <!-- Gallery Scene 4: Cheese Selection -->
  <defs>
    <linearGradient id="sceneGrad4" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fffde7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1000" height="1000" fill="url(#sceneGrad4)"/>
  
  <!-- Wooden board suggestion -->
  <ellipse cx="500" cy="700" rx="400" ry="60" fill="#8d6e63" opacity="0.2"/>
  <ellipse cx="500" cy="650" rx="380" ry="200" fill="#deb887" opacity="0.3"/>
  
  <!-- Cheese package 1 (left) -->
  <g transform="translate(120, 280)">
    <rect x="30" y="80" width="220" height="340" rx="12" fill="#ffffff" stroke="#e0e0e0" stroke-width="3"/>
    <rect x="30" y="80" width="220" height="90" rx="12" fill="#006633"/>
    <text x="140" y="130" font-family="Arial" font-size="26" font-weight="bold" fill="#ffffff" text-anchor="middle">CHEDDAR</text>
    <!-- Cheese slice illustration -->
    <path d="M 80 280 L 200 280 L 180 350 L 100 350 Z" fill="#ffeb3b" opacity="0.6"/>
  </g>
  
  <!-- Cheese package 2 (center) -->
  <g transform="translate(390, 300)">
    <rect x="30" y="70" width="200" height="310" rx="12" fill="#ffffff" stroke="#e0e0e0" stroke-width="3"/>
    <rect x="30" y="70" width="200" height="85" rx="12" fill="#006633"/>
    <text x="130" y="120" font-family="Arial" font-size="24" font-weight="bold" fill="#ffffff" text-anchor="middle">MOZZARELLA</text>
    <!-- Cheese illustration -->
    <circle cx="130" cy="280" r="50" fill="#f5f5f5" opacity="0.8"/>
  </g>
  
  <!-- Cream cheese tub (right) -->
  <g transform="translate(650, 360)">
    <ellipse cx="100" cy="280" rx="90" ry="22" fill="#e0e0e0"/>
    <rect x="10" y="130" width="180" height="150" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <ellipse cx="100" cy="130" rx="90" ry="22" fill="#ffffff" stroke="#e0e0e0" stroke-width="2"/>
    <rect x="30" y="170" width="140" height="90" rx="8" fill="#006633"/>
    <text x="100" y="215" font-family="Arial" font-size="22" font-weight="bold" fill="#ffffff" text-anchor="middle">CREAM</text>
    <text x="100" y="240" font-family="Arial" font-size="18" fill="#ffffff" text-anchor="middle">CHEESE</text>
  </g>
  
  <!-- Decorative cheese elements -->
  <circle cx="850" cy="200" r="40" fill="#ffeb3b" opacity="0.2"/>
  <path d="M 120 180 L 180 180 L 160 220 L 140 220 Z" fill="#ffeb3b" opacity="0.3"/>
  
  <!-- Text overlay -->
  <text x="500" y="900" font-family="Arial" font-size="32" font-weight="bold" fill="#006633" text-anchor="middle">Cheese Selection</text>
  <text x="500" y="950" font-family="Arial" font-size="20" fill="#666666" text-anchor="middle">AI-Generated Culinary Showcase</text>
</svg>
